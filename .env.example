# Environment variables for Structural Data Extraction Tool
# Copy this file to .env and fill in your API keys

# OpenAI API Key (for OpenAI service) - REQUIRED
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic/Claude API Key (for Claude service)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here

# Google/Gemini API Key (for Gemini service)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud Project ID (for Vertex AI service)
GOOGLE_CLOUD_PROJECT=your_google_cloud_project_id_here

# Ollama settings (for local Ollama service)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# Optional: Cache and output directories
CACHE_DIR=./.cache
OUTPUT_DIR=./output 